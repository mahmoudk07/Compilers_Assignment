digraph {
	S0 [shape=circle]
	S4 [shape=circle]
	S3 [shape=circle]
	S2 [shape=circle]
	S5 [shape=doublecircle]
	S1 [shape=circle]
	S0 -> S1 [label=a]
	S1 -> S2 [label=b]
	S2 -> S3 [label=c]
	S2 -> S4 [label=b]
	S2 -> S5 [label=d]
	S5 -> S5 [label=d]
	S4 -> S3 [label=c]
	S4 -> S4 [label=b]
	S4 -> S5 [label=d]
	S3 -> S3 [label=c]
	S3 -> S4 [label=b]
	S3 -> S5 [label=d]
	"" [shape=none]
	"" -> S0 [label=Start]
}
