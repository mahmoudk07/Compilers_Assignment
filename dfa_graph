digraph {
	S2 [shape=circle]
	S5 [shape=circle]
	S1 [shape=circle]
	S0 [shape=circle]
	S3 [shape=doublecircle]
	S4 [shape=circle]
	S0 -> S1 [label=a]
	S1 -> S2 [label=b]
	S2 -> S5 [label=c]
	S2 -> S4 [label=b]
	S2 -> S3 [label=d]
	S3 -> S3 [label=d]
	S4 -> S5 [label=c]
	S4 -> S4 [label=b]
	S4 -> S3 [label=d]
	S5 -> S5 [label=c]
	S5 -> S4 [label=b]
	S5 -> S3 [label=d]
	start -> S0
}
