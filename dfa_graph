digraph {
	"{2, 3, 4}" [shape=doublecircle]
	"{2, 3}" [shape=circle]
	"{1, 2}" [shape=circle]
	"{0, 3}" [shape=circle]
	"{1, 3, 4}" [shape=doublecircle]
	"{0, 3}" -> "{2, 3}" [label=a]
	"{0, 3}" -> "{1, 3, 4}" [label=b]
	"{1, 3, 4}" -> "{2, 3}" [label=a]
	"{1, 3, 4}" -> "{1, 2}" [label=b]
	"{1, 2}" -> "{2, 3}" [label=a]
	"{1, 2}" -> "{2, 3, 4}" [label=b]
	"{2, 3, 4}" -> "{2, 3}" [label=a]
	"{2, 3, 4}" -> "{1, 3, 4}" [label=b]
	"{2, 3}" -> "{2, 3}" [label=a]
	"{2, 3}" -> "{1, 3, 4}" [label=b]
}
