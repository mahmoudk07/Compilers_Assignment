digraph {
	"{3}" [shape=doublecircle]
	"{2}" [shape=circle]
	"{1}" [shape=circle]
	"{0}" [shape=circle]
	"{0}" -> "{3}" [label=a]
	"{0}" -> "{1}" [label=b]
	"{1}" -> "{0}" [label=a]
	"{1}" -> "{2}" [label=b]
	"{2}" -> "{3}" [label=a]
	"{2}" -> "{1}" [label=b]
	"{3}" -> "{2}" [label=a]
	"{3}" -> "{0}" [label=b]
}
